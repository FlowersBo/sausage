<!--pages/shopping_cart/myAddress/myAddress.wxml-->
<view class="myAddress">
  <block wx:for="{{address}}" wx:key="index">
    <view class="addressChild">
      <radio-group bindchange="radioChange">
        <radio value="{{item.id}}" checked="{{false}}" />
      </radio-group>
      <view style="margin-left: 20rpx;">
        <view style="font-weight: bold;font-size: 32rpx;margin-bottom: 20rpx;">北京温榆河分仓</view>
        <view style="font-size: 28rpx;">北京市朝阳区温榆河公园北区小河边上大柳树旁边</view>
      </view>
    </view>
  </block>
</view>